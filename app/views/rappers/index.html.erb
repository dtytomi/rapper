<h1>Listing rappers</h1>

<table>
  <thead>
    <tr>
      <th>Song</th>
      <th>Artist</th>
      <th>Album</th>
      <th>Description</th>
      <th>Picture</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rappers.each do |rapper| %>
      <tr>
        <td><%= rapper.song %></td>
        <td><%= rapper.artist %></td>
        <td><%= rapper.album %></td>
        <td><%= rapper.description %></td>
        <td><%= rapper.picture %></td>
        <td><%= rapper.rate %></td>
        <td><%= link_to 'Show', rapper %></td>
        <td><%= link_to 'Edit', edit_rapper_path(rapper) %></td>
        <td><%= link_to 'Destroy', rapper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rapper', new_rapper_path %>
